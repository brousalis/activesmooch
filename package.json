{
  "name": "activesmooch",
  "version": "0.0.1",
  "description": "ActiveCampaign + Smooch.io",
  "author": "Pete Brousalis",
  "engines": {
    "node": "8.4.0"
  },
  "index": "server.js",
  "scripts": {
